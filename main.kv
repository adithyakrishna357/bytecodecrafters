<LoginPage>:
    name :"first"
    Screen:
        BoxLayout:
            orientation: 'vertical'
            padding: dp(10)
            size_hint_y: None
            height: dp(640)

            canvas.before:
                Color:
                    rgba: 243/255, 240/255, 229/255, 1  # Background color #F3F0E5
                Rectangle:
                    size: self.size
                    pos: self.pos


            Image:
                source: "bytecodelogo2.png"
                size_hint: None, None
                width: "200dp"
                height: "200dp"
                pos_hint: {'center_x': 0.5, 'center_y': 0.84}


            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: dp(340)


                canvas.before:
                    Color:
                        rgba: 1, 1, 1, 1  # White background
                    RoundedRectangle:
                        size: self.width - dp(25), self.height  # Adjust the width
                        pos: self.x + dp(16), self.y  # Center horizontally
                        radius: [43]

                padding: dp(16)
                spacing: dp(16)

                MDTextField:
                    id:email
                    hint_text:"Email"
                    icon_right:'email'
                    font_size:"20dp"
                    font_name: "Aceh-Regular.otf"
                    size_hint_x:.88
                    theme_bg_color: "Custom"
                    fill_color_normal:rgba(243/255, 240/255, 229/255, 1)
                    fill_color_focus: 0.6, 0.6, 0.6, 1
                    hint_text_color_normal: 0.6, 0.6, 0.6, 1
                    hint_text_color_focus: 0, 0, 0, 1
                    text_color_normal: 0.6, 0.6, 0.6, 1 # Set the text color to full black
                    text_color_focus: 0, 0, 0, 1
                    line_color_normal: 0.5, 0.5, 0.5, 1  # Set the line color to full black
                    line_color_focus: 0, 0, 0, 1

                    icon_right_color_normal: 0.6, 0.6, 0.6, 1
                    icon_right_color_focus: 0, 0, 0, 1
                    pos_hint:{'center_x':.5,'center_y':.63}
                    on_text:self.text = self.text.replace(" ", "")
                    write_tab:False
                MDTextField:
                    id:psswrd
                    hint_text:"Password"
                    password:True
                    icon_right:'eye-off'
                    font_name: "Aceh-Regular.otf"
                    font_size:"20dp"
                    size_hint_x:.88
                    theme_bg_color: "Custom"
                    fill_color_normal:rgba(243/255, 240/255, 229/255, 1)
                    fill_color_focus: 0.6, 0.6, 0.6, 1
                    hint_text_color_normal: 0.6, 0.6, 0.6, 1
                    hint_text_color_focus: 0, 0, 0, 1
                    text_color_normal: 0.6, 0.6, 0.6, 1 # Set the text color to full black
                    text_color_focus: 0, 0, 0, 1
                    line_color_normal: 0.5, 0.5, 0.5, 1  # Set the line color to full black
                    line_color_focus: 0, 0, 0, 1

                    icon_right_color_normal: 0.6, 0.6, 0.6, 1
                    icon_right_color_focus: 0, 0, 0, 1
                    pos_hint:{'center_x':.5,'center_y':.5}
                    on_text:self.text = self.text.replace(" ", "")

                BoxLayout:
                    size_hint: .85, None
                    height: "30dp"
                    pos_hint: {'center_x': .5, 'center_y': .41}
                    spacing: "5dp"

                    MDCheckbox:
                        id: cb
                        size_hint: None, None
                        width: "30dp"
                        height: "30dp"
                        pos_hint: {'center_x': .5, 'center_y': .51}
                        theme_text_color: 'Custom'
                        color_inactive: 0.6, 0.6, 0.6, 1
                        on_press:
                            psswrd.password = not psswrd.password
                            psswrd.icon_right = "eye" if psswrd.icon_right == "eye-off" else "eye-off"

                    MDLabel:
                        text: "[ref=Show Password]Show Password[/ref]"
                        markup: True
                        font_name: "Fontspring-DEMO-sonnygothic-regular.otf"
                        pos_hint: {'center_x': .5, 'center_y': .51}
                        theme_text_color: "Custom"
                        text_color: 0.6, 0.6, 0.6, 1   # Set the text color to black
                        on_ref_press:
                            cb.active = not cb.active
                            psswrd.icon_right = "eye" if psswrd.icon_right == "eye-off" else "eye-off"

                BoxLayout:
                    size_hint:None,None
                    height:"30dp"
                    pos_hint:{'center_x':0.46,'center_y':.32}
                    spacing: "5dp"
                    MDFlatButton:
                        text: "SIGN IN"
                        font_style: "Button"
                        font_size: "24dp"
                        md_bg_color: 0.6, 0.6, 0.6, 1
                        theme_text_color: "Custom"
                        font_name: "Fontspring-DEMO-sonnygothic-regular.otf"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        on_release:
                            root.on_sign_in(email.text, psswrd.text)
                BoxLayout:
                    size_hint:None, None
                    height:"30dp"
                    pos_hint:{'center_x':0.49,'center_y':.26}
                    spacing: "5dp"
                    MDTextButton:
                        text:"Forgot Password?"
                        font_style: "Caption"
                        font_name: "Aceh-Regular.otf"
                        theme_text_color: "Custom"
                        text_color: 0.6, 0.6, 0.6, 1
                        font_size: "12.5dp"
                        on_release: root.sendotp()
                        pos_hint: {'center_x': 0.5, 'center_y': 0.45}
            Widget:
                size_hint_y: None
                height: dp(9)  # Adjust the space between the button and the label
            BoxLayout:
                orientation: 'vertical'
                size_hint: None, None
                height: "30dp"  # Adjust the height as needed
                pos_hint: {'center_x': 0.51, 'center_y': 0.1}
                spacing: "5dp"

                MDTextButton:
                    text: "SIGNUP"
                    font_style: "Button"
                    font_size:"16dp"
                    font_name: "Fontspring-DEMO-sonnygothic-regular.otf"
                    theme_text_color: "Custom"
                    text_color: 0.6, 0.6, 0.6, 1
                    on_release: app.root.current = 'second'
            Widget:
                size_hint_y: None
                height: dp(9)  # Adjust the space between the button and the label

            BoxLayout:
                size_hint_y: None
                orientation: 'horizontal'
                height: dp(50)

                spacing: dp(25)
                pos_hint: {'center_x': 0.8, 'center_y': 0.1}

                MDIconButton:
                    icon: "instagram"
                    theme_text_color: "Custom"
                    icon_color: 0.6, 0.6, 0.6, 1
                    size_hint: None, None
                    size: dp(0.1), dp(0.1)
                    on_release: app.open_url5()  # Update the on_release function as needed

                MDIconButton:
                    icon: "whatsapp"
                    theme_text_color: "Custom"
                    icon_color: 0.6, 0.6, 0.6, 1
                    size_hint: None, None
                    size: dp(0.1), dp(0.1)
                    on_release: app.open_url4()  # Update the on_release function as needed






